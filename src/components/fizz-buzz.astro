---
const numbers = [];

for (let i = 1; i <= 100; i++) {
  const element = i;
  if (element % 3 == 0 && element % 5 == 0) {
    numbers.push('FizzBuzz');
  } else if (element % 3 == 0) {
    numbers.push('Fizz');
  } else if (element % 5 == 0) {
    numbers.push('Buzz');
  } else {
    numbers.push(element);
  }
}
---

<maim>
  <section>
    {
      numbers.map((item) => {
        return (
          <>
            <span class="size-1 text-falcon-50">{item}</span>
          </>
        );
      })
    }
  </section>
</maim>
